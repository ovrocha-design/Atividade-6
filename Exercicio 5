public class Exercicio5 {
    public static void main(String[] args) {
        int numero1 = 5;
        int vezes = 1;
        while (vezes <= 10) {
            int resultado = numero1 * vezes;
            System.out.println(numero1 + " x " + vezes + " = " + resultado);
            vezes++;
        }
    }
}
